    <group id="1" isSynthetic="true">
        <label>Page1</label>
        <select1 bind="visit">
            <label>1. Type of visit</label>
            <item id="baseline">
                <label>School Baseline Visit</label>
                <value>baseline</value>
            </item>
            <item id="update">
                <label>School Information Update</label>
                <value>update</value>
            </item>
        </select1>
        <input bind="select_school"
               appearance="ex:org.odk.collect.android.activities.entities.PrefillActivity(table_name='schools',returnFld='school;address;subcounty;district;__code;')">
            <label>2. Select the school</label>
            <hint>
                json:{"layout":{"appearance":"ex:org.odk.collect.android.activities.entities.PrefillActivity(table_name='schools',returnFld='school;address;subcounty;district;__code;')"}}
            </hint>
        </input>
        <group id="3" appearance="field-list">
            <label/>
            <hint>json:{"layout":{"appearance":"field-list"}}</hint>
            <input bind="visit_date">
                <label>3. Date of Visit</label>
            </input>
            <input bind="interviewer">
                <label>4. Name Name of Interviewer</label>
            </input>
            <input bind="interviewer_mobile">
                <label>5. Mobile Number</label>
            </input>
        </group>
        <group id="4" appearance="field-list">
            <label/>
            <hint>json:{"layout":{"appearance":"field-list"}}</hint>
            <input bind="school_name">
                <label>6. School Name:</label>
            </input>
            <input bind="address">
                <label>7. School Address:</label>
            </input>
            <input bind="phone">
                <label>8. School Telephone</label>
            </input>
            <input bind="email">
                <label>9. Email</label>
            </input>
        </group>
        <select1 bind="district" appearance="minimal w4">
            <label>10. District</label>
            <hint>json:{"layout":{"appearance":"minimal w4"}}</hint>
            <item id="jinja">
                <label>Jinja</label>
                <value>jinja</value>
