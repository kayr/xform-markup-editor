              required="true()"/>
        <bind id="day" nodeset="/iics_survey_school_questionnaire_v1/enrolment/day" type="xsd:int" required="true()"/>
        <bind id="total_enrolment" nodeset="/iics_survey_school_questionnaire_v1/enrolment/total_enrolment"
              type="xsd:int" required="true()" locked="true()"
              calculate="/iics_survey_school_questionnaire_v1/enrolment/boarding + /iics_survey_school_questionnaire_v1/enrolment/day"/>
        <bind id="teacher_no" nodeset="/iics_survey_school_questionnaire_v1/enrolment/teacher_no" type="xsd:int"
              required="true()"/>
        <bind id="non_teacher_no" nodeset="/iics_survey_school_questionnaire_v1/enrolment/non_teacher_no" type="xsd:int"
              required="true()"/>
        <bind id="total_traff" nodeset="/iics_survey_school_questionnaire_v1/enrolment/total_traff" type="xsd:int"
              required="true()" locked="true()"
