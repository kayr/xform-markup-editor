              required="true()"/>
        <bind id="interviewer" nodeset="/iics_survey_school_questionnaire_v1/interviewer" type="xsd:string"
              required="true()"/>
        <bind id="interviewer_mobile" nodeset="/iics_survey_school_questionnaire_v1/interviewer_mobile"
              type="xsd:string" required="true()" constraint="regex(., &quot;^07[0-9]{8}$&quot;)"
              message="&quot;Please enter a valid phone number. e.g. 0778345678&quot;"/>
        <bind id="school_name" nodeset="/iics_survey_school_questionnaire_v1/school_name" type="xsd:string"
              required="true()"/>
        <bind id="address" nodeset="/iics_survey_school_questionnaire_v1/address" type="xsd:string" required="true()"/>
        <bind id="phone" nodeset="/iics_survey_school_questionnaire_v1/phone" type="xsd:string"
              relevant="/iics_survey_school_questionnaire_v1/visit = 'baseline'" action="show"
              constraint="regex(., &quot;^[0-9]{10}$&quot;)"
              message="&quot;Please enter a valid phone number. e.g. 0778345678&quot;" required="true()"/>
        <bind id="email" nodeset="/iics_survey_school_questionnaire_v1/email" type="xsd:string"
              relevant="/iics_survey_school_questionnaire_v1/visit = 'baseline'" action="show"
              constraint="regex(., '[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,5}')" message="put a valid email"
              required="false()"/>
        <bind id="district" nodeset="/iics_survey_school_questionnaire_v1/district" type="xsd:string"
              relevant="/iics_survey_school_questionnaire_v1/visit = 'baseline'" action="show" required="true()"/>
